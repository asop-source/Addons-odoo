<odoo>
	<data>
		<record id="view_ram_tree" model="ir.ui.view">
			<field name="name">view_ram.tree</field>
			<field name="model">master.ram</field>
			<field name="type">tree</field>
			<field name="priority" eval="8"/>
			<field name="arch" type="xml">
				<tree string="Master Ram">
                    <field name="kode"/>
                    <field name="name" />
                </tree>
            </field>
		</record>

		<record id="view_ram_form" model="ir.ui.view">
			<field name="name">view_ram.form</field>
			<field name="model">master.ram</field>
			<field name="type">form</field>
			<field name="priority" eval="8"/>
			<field name="arch" type="xml">
				<form string="Master Ram">
				<sheet>
					<group>
		                    <field name="kode"/>
		                    <field name="name" />
              		</group>
                </sheet>
                </form>
            </field>
		</record>

	</data>
</odoo>