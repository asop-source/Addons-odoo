<odoo>
	<data>
		<record id="view_disk_tree" model="ir.ui.view">
			<field name="name">view_disk.tree</field>
			<field name="model">master.disk</field>
			<field name="type">tree</field>
			<field name="priority" eval="8"/>
			<field name="arch" type="xml">
				<tree string="Master DISK">
                    <field name="kode"/>
                    <field name="name" />
                </tree>
            </field>
		</record>

		<record id="view_disk_form" model="ir.ui.view">
			<field name="name">view_disk.form</field>
			<field name="model">master.disk</field>
			<field name="type">form</field>
			<field name="priority" eval="8"/>
			<field name="arch" type="xml">
				<form string="Master DISK">
					<sheet>
						<group>
		                    <field name="kode"/>
		                    <field name="name" />
		                </group>
		            </sheet>
                </form>
            </field>
		</record>

	</data>
</odoo>