<odoo>
	<data>
		<record id="view_spesifikasi_tree" model="ir.ui.view">
			<field name="name">view_spesifikasi.tree</field>
			<field name="model">master.posisi</field>
			<field name="type">tree</field>
			<field name="priority" eval="8"/>
			<field name="arch" type="xml">
				<tree string="Master Spesifikasi">
                    <field name="kode"/>
                    <field name="name" />
                </tree>
            </field>
		</record>

		<record id="view_spesifikasi_form" model="ir.ui.view">
			<field name="name">view_spesifikasi.form</field>
			<field name="model">master.spesifikasi</field>
			<field name="type">form</field>
			<field name="priority" eval="8"/>
			<field name="arch" type="xml">
				<form string="Master Spesifikasi">
				<sheet>
					<group>
		                    <field name="kode"/>
		                    <field name="name" />
              		</group>
                </sheet>
                </form>
            </field>
		</record>

	</data>
</odoo>