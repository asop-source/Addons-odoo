<odoo>
	<data>
		<record id="view_merk_tree" model="ir.ui.view">
			<field name="name">view_merk.tree</field>
			<field name="model">management.merk</field>
			<field name="type">tree</field>
			<field name="priority" eval="8"/>
			<field name="arch" type="xml">
				<tree string="Master Merk">
                    <field name="kode"/>
                    <field name="name" /> 
                </tree>
            </field>
		</record>

		<record id="view_merk_form" model="ir.ui.view">
			<field name="name">view_merk.form</field>
			<field name="model">management.merk</field>
			<field name="type">form</field>
			<field name="priority" eval="8"/>
			<field name="arch" type="xml">
				<form string="Master Merk">
				<sheet>
					<group>
		                    <field name="kode"/>
		                    <field name="name" />
              		</group>
                </sheet>
                </form>
            </field>
		</record>

	</data>
</odoo>