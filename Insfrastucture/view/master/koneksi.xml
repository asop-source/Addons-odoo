<odoo>
	<data>
		<record id="view_koneksi_tree" model="ir.ui.view">
			<field name="name">view_koneksi.tree</field>
			<field name="model">master.koneksi</field>
			<field name="type">tree</field>
			<field name="priority" eval="8"/>
			<field name="arch" type="xml">
				<tree string="Master Connection">
                    <field name="kode"/>
                    <field name="name" />
                </tree>
            </field>
		</record>

		<record id="view_koneksi_form" model="ir.ui.view">
			<field name="name">view_koneksi.form</field>
			<field name="model">master.koneksi</field>
			<field name="type">form</field>
			<field name="priority" eval="8"/>
			<field name="arch" type="xml">
				<form string="Master Connection">
					<sheet>
						<group>
		                    <field name="kode"/>
		                    <field name="name" />
		                </group>
            	    </sheet>
                </form>
            </field>
		</record>

	</data>
</odoo>