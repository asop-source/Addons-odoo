<odoo>
	<data>
		<record id="view_location_tree" model="ir.ui.view">
			<field name="name">view_location.tree</field>
			<field name="model">master.location</field>
			<field name="type">tree</field>
			<field name="priority" eval="8"/>
			<field name="arch" type="xml">
				<tree string="Master Location">
                    <field name="kode"/>
                    <field name="name" />
                </tree>
            </field>
		</record>

		<record id="view_location_form" model="ir.ui.view">
			<field name="name">view_location.form</field>
			<field name="model">master.location</field>
			<field name="type">form</field>
			<field name="priority" eval="8"/>
			<field name="arch" type="xml">
				<form string="Master Location">
				<sheet>
					<group>
		                    <field name="kode"/>
		                    <field name="name" />
              		</group>
                </sheet>
                </form>
            </field>
		</record>

	</data>
</odoo>