<odoo>
    <data>

      <record id="view_management_profile_form" model="ir.ui.view">
            <field name="name">management.maintenance.form</field>
            <field name="model">management.maintenance</field>
            <field name="type">form</field>
            <field name="priority" eval="8"/>
            <field name="arch" type="xml">
                <form string="Maintenance">
                  <sheet>
                        <div class="oe_title">
                            <label for="tiket" class="oe_edit_only" string="No Tiket"/>
                            <h1>
                                <field name="tiket"/>
                            </h1>
                        </div>
                    <group>
                            <field name="tujuan" />
                            <field name="start_date" />
                            <field name="status" />
                            <field name="keterangan"/>
                    </group>
                 </sheet>
                </form>
            </field>
        </record>

    </data>
</odoo>